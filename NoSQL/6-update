// Updates all documents in the 'school' collection where name is "Holberton school".
// It adds the attribute 'address' with the value "972 Mission street".
db.school.updateMany(
{ name: "Holberton school" }, // Filter: match documents where name is "Holberton school"
{ $set: { address: "972 Mission street" } } // Update operation: use $set to add/modify the address field
)
